<div class="row">
  <div class="col-md-12">
    <div class="card">
      <table class="table table-no-margin-bottom">
        <thead class="thead-light">
        <th>Rank</th>
        <th>Name</th>
        <th>Total Supply</th>
        <th>Price Tracker</th>
        </thead>
        <tbody>
        {{#each coins as |coin|}}
          {{card-coin coin=coin tagName='tr'}}
        {{/each}}
        </tbody>
      </table>
    </div>
    <br>
    <div class="col-md-12">
      <nav aria-label="Page navigation example">
        <ul class="pagination justify-content-center">
          <li class="page-item"><a class="page-link" href="#" {{action 'previous'}}>Prev</a></li>
          {{#each pageRange as |page|}}
            <li class="page-item"><a class="page-link" href="#" {{action 'goToPage' page}}>{{page}}</a></li>
          {{/each}}
          <li class="page-item"><a class="page-link" href="#" {{action 'next'}}>Next</a></li>
        </ul>
      </nav>
    </div>

  </div>
</div>
{{outlet}}